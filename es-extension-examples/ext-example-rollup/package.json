{
    "name": "@gots/extension-example-rollup",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "scripts": {
        "start": "DO_SERVE=true rollup --config --watch",
        "build": "rollup --config",
        "maybe-install": "[ -d node_modules ] && [ -z $DO_INSTALL ] || npm install",
        "container": "npm run maybe-install && DO_SERVE=true rollup --config --watch"
    },
    "devDependencies": {
        "@gots/es-runtime": "^1.10.0",
        "@rollup/plugin-commonjs": "^18.0.0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "rollup": "^2.45.2",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    }
}
