{
  "name": "@gots/es-common-extensions",
  "version": "2.12.0",
  "private": true,
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "maybe-install": "[ -d node_modules ] && [ -z $DO_INSTALL ] || npm install",
    "container": "npm run maybe-install && snowpack dev"
  },
  "dependencies": {
    "@gots/noowf-inter-widget-communication": "^2.6.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-sparklines": "^1.7.0"
  },
  "devDependencies": {
    "@gots/es-runtime": "workspace:*",
    "@snowpack/plugin-dotenv": "~2.2.0",
    "@snowpack/plugin-react-refresh": "~2.5.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.2.0",
    "@testing-library/react": "^11.2.7",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^8.2.3",
    "@types/prismjs": "^1.16.6",
    "@types/react": "~17.0.3",
    "@types/react-dom": "~17.0.3",
    "@types/react-sparklines": "^1.7.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/snowpack-env": "~2.3.4",
    "@web/test-runner": "~0.13.28",
    "chai": "~4.3.6",
    "prettier": "^2.5.1",
    "snowpack": "^3.8.8",
    "typescript": "^4.7.2"
  }
}
