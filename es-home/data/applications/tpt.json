{
    "name": "tpt",
    "title": "Tactical Planning Tool",   
    "header": [
        "es.common.classification.banner.header"
    ],
    "extensions": [
        "/ui/dist/extensions/console-extension.js",
        "/ui/common/dist/debug-metrics.js",
        "/es/common/dist/classification-banner.js"
    ],
    "iframes": [
        {
            "id": "tpt.mopc",
            "title": "MOPC",
            "location": "center",
            "iframeSource": "/geoviz/",
            "resizeHandle": true
        },
        {
            "id": "tpt.timeline",
            "title": "TPT Timeline",
            "location": "bottom",
            "iframeSource": "/tpt#?hide-navbar=true",
            "resizeHandle": true
        },
        {
            "id": "tpt.geotree",
            "title": "Geo Tree",
            "location": "left",
            "iframeSource": "/geotree/",
            "resizeHandle": true
        }
    ]
}
